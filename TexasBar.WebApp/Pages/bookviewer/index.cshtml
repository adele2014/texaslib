<body data-manual-id="5">
    <div class="container">

        <!--Updated to Header Tag, added .box_shadow-->
        <header class="masthead box_shadow">
            <!--Added Div for logo thumb-->
            <div class="tbb_thumb_logo"></div>

            <form class="search_form">
                <input type="text" id="search_field" name="search" placeholder="search" class="search">
                <button id="search_btn"><img src="../images/search.png"></button>
            </form>
            <a href="#" class="menu"><img src="../images/interface/chapters.png" alt="Main Menu"><span>Main Menu</span></a>
            <a href="#" class="bookmark"><img src="../images/interface/bookmark.png" alt="Bookmark Page"><span>Bookmark Page</span></a>
            <a asp-page="/Index" class="my_account" target="_blank"><img src="../images/interface/account.png" alt="Account Page"><span>My Account</span></a>
        </header>

        <!--Updated to Aside Tag-->
        <aside class="sidebar">
            <div class="close_side"></div>
            <img class="sidebar_logo" src="../images/interface/TBO-side-logo.jpg">
            <p><a href="../bookmarks.html">Bookmarks</a></p>
            <p><a href="../articles.html">CLE Articles</a></p>
            <p id="changes_doc_nav"><a asp-page="/Update">Recent Updates</a></p>
            <ul class="chapter_group_nav">
                <li class="current">
                    <a href="#">Chapters</a>
                    <ul>

                        @foreach (var item in Model.chapters)
                        {
                            <li class="">
                                <a asp-page="/bookviewer/version 1/TBO/Chapter 1">
                                    @item.ChapterNo &nbsp; @item.Description
                                </a>
                            </li>
                        }

                        @Model.chaptersTempString

                    </ul>
                </li>
            </ul>
        </aside>

        <div class="sub_sidebar">
            <div class="header box_shadow">
                <p class="chapter_number">@Model.chapterNo</p>
                <!-- Not needed. Design does not include this.
                <p class="chapter_title">Attorney-Client Relationship and Communications</p>
                -->
            </div>
            <div class="sub_nav_container form">
                <div class="ctrls_container box_shadow">
                    <a id="practice_notes_ctrl" class="sub_ctrl" href="#">Practice Notes</a>
                    <a id="forms_ctrl" class="sub_ctrl" href="#">Forms</a>
                </div>
                <ul class="section_nav">
                    @Model.anchorMenuHtmlString
                    <li><a href="#">This chapter only has forms.</a></li>
                </ul>
                <ul class="form_nav">
                    @Model.subMenuHtmlString


                </ul>
            </div>
        </div>
        <!--FM content Goes here-->
        <div class="content">
            <div class="content_nav"> </div>
            <div class="chapter_content">
                @if (1 == 1)
                {
                    @Model.htmlString
                }
                else
                {
                    <div dir="ltr">
                        <p dir="ltr" class="FM_TitleChapter" style="font-weight : normal;"><span style="font-size  : 11.0pt; "></span><span class="FM_Bold">Chapter 1<br></span>Ownership</p>
                        <h2>SORRY YOU ARE NOT ALLOW TO READ THIS CONTENT</h2>
                    </div>

                }

                <div dir="ltr">
                    <p dir="ltr" class="FM_Body"><a name="HT_TMS_C29x1176910"></a>&#xA0;</p>
                </div>









            </div>
        </div>
        <!--End FM content Goes here-->
        <!--Updated with Footer Tag-->
        <footer class="footer">
            <p>Contact Us: 512-427-1499 or <a href="mailto:books@texasbar.com">books@texasbar.com</a> | &#xA9; 2019 State Bar of Texas</p>
        </footer>
    </div>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/texas/site.js"></script>
    <script src="~/js/texas/jquery.waypoints.min.js"></script>
    <script src="~/js/texas/annotator.min.js"></script>
    <script src="~/js/texas/annotator.store.min.js"></script>
    <script src="~/js/texas/main.js"></script>
    <script src="~/js/texas/bookmarks.js"></script>
    <script src="~/js/texas/search.js"></script>
    <script src="~/js/texas/changes.js"></script>
    <script>

    div.innerHTML = response;
    var scripts = div.getElementsByTagName('script');
    for (var ix = 0; ix < scripts.length; ix++) {
    eval(scripts[ix].text);
    }
    </script>


    @*<script>window.location.href = '../3_Ownership/Form3-1.htm'</script>*@
</body>
</html>